{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "lib": ["ESNEXT", "DOM"]
  }
}
